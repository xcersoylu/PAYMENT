    "AS4DATE":"0000-00-00",
    "AS4TIME":"00:00:00",
    "SOURCE":"@AbapCatalog.viewEnhancementCategory: [#NONE]\r\n@AccessControl.authorizationCheck: #NOT_REQUIRED\r\n@EndUserText.label: 'Ödeme Listesi Verisi'\r\n@Metadata.ignorePropagatedAnnotations: true\r\n@ObjectModel.usageType:{\r\n    serviceQuality: #X,\r\n    sizeCategory: #S,\r\n    dataClass: #MIXED\r\n}\r\ndefine view entity YI_PYM_DDL_PAY_cus_ALL_DATA\r\n  as select from ypym_t_pay_cus as payment inner join I_Customer on I_Customer.Customer = payment.customer\r\n{\r\n  key payment.paymentnumber               as PaymentNumber,\r\n  key payment.accountingdocument          as AccountingDocument,\r\n  key payment.fiscalyear                  as FiscalYear,\r\n  key payment.accountingdocumentitem      as AccountingDocumentItem,\r\n      @EndUserText.label: 'Liste Tarihi'\r\n      payment.createdate                  as Listdate,\r\n//      payment.paymentdate                 as PaymentDate,\r\n      payment.customer                    as Customer,\r\n      I_Customer.CustomerName             as CustomerName,\r\n      payment.companycode                 as CompanyCode,\r\n      payment.documentdate                as DocumentDate,\r\n      @Semantics.amount.currencyCode: 'TransactionCurrency'\r\n      @EndUserText.label: 'İşlem PB Cinsinden Tutar'\r\n      payment.amountintransactioncurrency as AmountInTransactionCurrency,\r\n      @EndUserText.label: 'İşlem PB'\r\n      payment.transactioncurrency         as TransactionCurrency,\r\n      @Semantics.amount.currencyCode: 'CompanyCodeCurrency'\r\n      @EndUserText.label: 'Şirket Kodu PB Cinsinden Tutar'\r\n      payment.amountincompanycodecurrency as AmountInCompanyCodeCurrency,\r\n      @EndUserText.label: 'Şirket Kodu PB'\r\n      payment.companycodecurrency         as CompanyCodeCurrency,\r\n      @Semantics.amount.currencyCode: 'AdditionalCurrency1'\r\n      @UI.hidden: true\r\n      payment.amountinadditionalcurrency1 as AmountInAdditionalCurrency1,\r\n      @UI.hidden: true\r\n      payment.additionalcurrency1         as AdditionalCurrency1,\r\n      @Semantics.amount.currencyCode: 'AdditionalCurrency2'\r\n      @UI.hidden: true\r\n      payment.amountinadditionalcurrency2 as AmountInAdditionalCurrency2,\r\n      @UI.hidden: true\r\n      payment.additionalcurrency2         as AdditionalCurrency2,\r\n      @Semantics.amount.currencyCode: 'Currency'\r\n      payment.paymentamount               as PaymentAmount,\r\n      @EndUserText.label: 'Ödeme Tutarı PB'\r\n      payment.currency                    as Currency,\r\n      payment.paymentexplanation          as PaymentExplanation,\r\n      payment.receiptexplanation          as ReceiptExplanation,\r\n      payment.customeriban                as Iban,\r\n      payment.companybankcode             as BankCode,\r\n      payment.companyaccountcode          as AccountCode,\r\n      payment.paymentblockingreason       as PaymentClockingReason\r\n      \r\n}\r\n/*+[internal] {\n\"BASEINFO\":\n{\n\"FROM\":\n[\n\"I_CUSTOMER\",\n\"YPYM_T_PAY_CUS\"\n],\n\"ASSOCIATED\":\n[],\n\"BASE\":\n[],\n\"ANNO_REF\":\n[],\n\"SCALAR_FUNCTION\":\n[],\n\"VERSION\":0,\n\"ANNOREF_EVALUATION_ERROR\":\"\"\n}\n}*/",
