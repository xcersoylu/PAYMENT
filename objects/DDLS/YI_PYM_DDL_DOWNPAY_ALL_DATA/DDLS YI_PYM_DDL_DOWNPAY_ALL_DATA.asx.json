    "SOURCE":"@AbapCatalog.viewEnhancementCategory: [#NONE]\r\n@AccessControl.authorizationCheck: #NOT_REQUIRED\r\n@EndUserText.label: 'Ödeme Listesi Verisi'\r\n@Metadata.ignorePropagatedAnnotations: true\r\n@ObjectModel.usageType:{\r\n    serviceQuality: #X,\r\n    sizeCategory: #S,\r\n    dataClass: #MIXED\r\n}\r\ndefine view entity YI_PYM_DDL_downpay_ALL_DATA\r\n  as select from ypym_t_downpay as downpay\r\n    inner join   I_Supplier on I_Supplier.Supplier = downpay.supplier\r\n{\r\n  key downpay.paymentnumber      as Paymentnumber,\r\n  key downpay.purchaseorder      as Purchaseorder,\r\n  key downpay.purchaseorderitem  as Purchaseorderitem,\r\n      @EndUserText.label: 'Liste Tarihi'\r\n      downpay.createdate         as Listdate,  \r\n//      downpay.paymentdate        as Paymentdate,\r\n      downpay.supplier           as Supplier,\r\n      I_Supplier.SupplierName    as SupplierName,\r\n      downpay.companycode        as Companycode,\r\n      @Semantics.amount.currencyCode: 'currency'\r\n      downpay.paymentamount      as Paymentamount,\r\n      @EndUserText.label: 'Ödeme Tutarı PB'\r\n      downpay.currency           as Currency,\r\n      downpay.paymentexplanation as Paymentexplanation,\r\n      downpay.receiptexplanation as Receiptexplanation,\r\n      downpay.supplieriban       as Supplieriban,\r\n      downpay.companybankcode    as Companybankcode,\r\n      downpay.companyaccountcode as Companyaccountcode,\r\n      downpay.paymentrequestdate as Paymentrequestdate\r\n}\r\n/*+[internal] {\n\"BASEINFO\":\n{\n\"FROM\":\n[\n\"I_SUPPLIER\",\n\"YPYM_T_DOWNPAY\"\n],\n\"ASSOCIATED\":\n[],\n\"BASE\":\n[],\n\"ANNO_REF\":\n[],\n\"SCALAR_FUNCTION\":\n[],\n\"VERSION\":0,\n\"ANNOREF_EVALUATION_ERROR\":\"\"\n}\n}*/",
